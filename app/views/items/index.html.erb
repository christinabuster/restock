<p style="color: green"><%= notice %></p>

<h1>Seed Bank Inventory</h1>

<div class="grid-container">
<%= link_to "New item", new_item_path %>

  <div id="items">
    <% @items.each do |item| %>
      <%= render item %>
      <p>
        <%= link_to "Show this item", item %>
      </p>
    <% end %>
  </div>
</div>
